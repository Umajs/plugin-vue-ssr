{
  "name": "@umajs/plugin-vue-ssr",
  "version": "2.0.1",
  "description": "In umajs, Vue is used to develop the plug-in of spa and MPa, which supports server-side rendering and client-side rendering",
  "author": "zunyi_zjj@163.com",
  "keywords": [
    "vue",
    "ssr",
    "vue ssr",
    "Nuxtjs",
    "Nuxt",
    "umajs",
    "umajs-vue-ssr",
    "@umajs/plugin-vue-ssr"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "fix": "esw src --fix --ext .ts",
    "lint": "npx eslint src --ext .ts",
    "lint-w": "esw src --clear --color -w --ext .ts",
    "build-w": "tsc -w --inlineSourceMap",
    "start": "run-p lint-w build-w",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@srejs/vue": "^1.3.0",
    "consolidate": "^0.16.0",
    "get-stream": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/koa": "^2.0.48",
    "@types/node": "^12.20.16",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@umajs/core": "^2.0.1",
    "@umajs/router": "^2.0.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-watch": "^7.0.0",
    "koa": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Umajs/plugin-vue-ssr.git"
  },
  "bugs": {
    "url": "https://github.com/Umajs/plugin-vue-ssr"
  },
  "homepage": "https://github.com/Umajs/plugin-vue-ssr#readme"
}
